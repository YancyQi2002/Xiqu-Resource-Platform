{
  "name": "xiqu-resource-platform",
  "private": true,
  "version": "0.0.0",
  "license": "GPL-3.0-or-later",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "tauri": "tauri dev"
  },
  "lint-staged": {
    "*.{js,vue,jsx,tsx}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-git"
    }
  },
  "dependencies": {
    "@tauri-apps/api": "^1.0.0-rc.1",
    "husky": "^7.0.4",
    "vue": "^3.2.25"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@tailwindcss/forms": "^0.5.0",
    "@tauri-apps/cli": "^1.0.0-rc.5",
    "@vitejs/plugin-vue": "^2.2.0",
    "autoprefixer": "^10.4.2",
    "commitizen": "^4.2.4",
    "cz-git": "^1.2.1",
    "lint-staged": "^12.3.5",
    "postcss": "^8.4.8",
    "postcss-import": "^14.0.2",
    "prettier": "^2.5.1",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.5.4",
    "vite": "^2.8.0",
    "vue-tsc": "^0.29.8"
  }
}